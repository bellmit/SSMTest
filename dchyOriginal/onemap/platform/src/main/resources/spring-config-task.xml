<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="schedulerFactoryBean" class="org.springframework.scheduling.quartz.SchedulerFactoryBean">
        <property name="schedulerName" value="captureScheduler"></property>
    </bean>

    <bean id="quartzScheduleManager" class="cn.gtmap.onemap.platform.service.impl.QuartzScheduleManagerImpl"
          init-method="init">
        <property name="jobs" value="${egov.conf}/omp/quartzjobs.yml"/>
        <property name="startupDelay" value="10"/>
    </bean>
</beans>