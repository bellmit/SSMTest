package cn.gtmap.msurveyplat.common.domain.dchyxmgl;

import javax.persistence.Id;
import javax.persistence.Table;
import java.util.Date;

/**
 * @author <a href="mailto:lijun1@gtmap.cn">lijun1</a>
 * @version 1.0, 2020/11/27 11:06
 * @description 多测合一用户消息
 */
@Table(name = "DCHY_XMGL_YHXX")
public class DchyXmglYhxx {
    @Id
    private String yhxxid; //用户消息id
    private String jsyhid; //接收用户id
    private String glsxid; //关联事项id
    private String xxnr; //消息内容
    private String fsyhid; //发送用户id
    private String dqzt; //读取状态(字典表A.10)
    private Date fssj; //发送时间
    private Date dqsj; //读取时间
    private String xxlx; //消息类型(字典表A.29)
    private String sftz; //是否跳转

    public String getSftz() {
        return sftz;
    }

    public void setSftz(String sftz) {
        this.sftz = sftz;
    }

    public String getYhxxid() {
        return yhxxid;
    }

    public void setYhxxid(String yhxxid) {
        this.yhxxid = yhxxid;
    }

    public String getJsyhid() {
        return jsyhid;
    }

    public void setJsyhid(String jsyhid) {
        this.jsyhid = jsyhid;
    }

    public String getGlsxid() {
        return glsxid;
    }

    public void setGlsxid(String glsxid) {
        this.glsxid = glsxid;
    }

    public String getXxnr() {
        return xxnr;
    }

    public void setXxnr(String xxnr) {
        this.xxnr = xxnr;
    }

    public String getFsyhid() {
        return fsyhid;
    }

    public void setFsyhid(String fsyhid) {
        this.fsyhid = fsyhid;
    }

    public String getDqzt() {
        return dqzt;
    }

    public void setDqzt(String dqzt) {
        this.dqzt = dqzt;
    }

    public Date getFssj() {
        return fssj;
    }

    public void setFssj(Date fssj) {
        this.fssj = fssj;
    }

    public Date getDqsj() {
        return dqsj;
    }

    public void setDqsj(Date dqsj) {
        this.dqsj = dqsj;
    }

    public String getXxlx() {
        return xxlx;
    }

    public void setXxlx(String xxlx) {
        this.xxlx = xxlx;
    }
}
