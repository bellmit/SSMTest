html, body {	height: 100%;}.container {	width: 100%;}#footer {	border-radius: 0;	border-top: 1px solid #cdcdcd;	background-color: transparent;	color: #888888;	margin: 20px;}/*=== Ctn ===*/#ctn {	margin: 0;	padding: 0;}/*=== Toolbar ===*/#ctn .toolbar {	background-color: #FFF;	border-bottom: 1px solid #cdcdcd;	padding: 6px 0;	height: 20px;	position: relative;}#ctn .toolbar .nav {	display: inline-block;}#ctn .toolbar .breadcrumb > li > a{	color: #222;}#ctn .toolbar .buttons {	position: absolute;	right: 5px;	top: 5px;}/*=== Leftbar ===*/#leftBar {	float: left;	position: relative;}#leftBar .service-overview {	width: 350px;}.leftbar-ctn {	background-color: #F7F7F7;	border-right: 1px solid #cdcdcd;}.data-list-wrap{	margin: 0 30px 0 30px;}.data-list-wrap span,.data-list-wrap label{	display: inline-block;	margin-bottom: 10px;}.data-list-wrap label{	color: #272727;	width: 80px;}.data-list-wrap span{	color: #0F1746;}.leftbar-ctn .title{	padding: 0 30px 20px 20px;}/*=== title ===*/.section-title {	color: #171717;	font-family: 'microsoft yahei';}/*=== Main ===*/#main {	background-color: white;}.main-ctn {	background-color: white;	padding: 0 10px;}.map-ctn {	background: url(../images/map.jpg) 0 -35px no-repeat;}.service-panel-wrapper {	border: 1px solid #cdcdcd;	margin: 30px 30px 30px 0;	padding: 30px;}table td.drag-area {	cursor: move;}table .sort_placeholder div {	border: 2px dashed #124278;}.final-panel {	position: absolute;	z-index: 500;	background: #EAEAEA;	border: 1px solid #CDCDCD;	border-radius: 4px;	box-shadow: -1px 2px 3px rgba(0,0,0,.2);	top: 50%;	right: 50%;	padding: 15px 15px 15px 15px;	display: none;}.final-panel.fix-top {	top: 90px;	right: 30px;	padding: 5px 15px 10px 15px;	border-top: none;	border-radius: 0 0 4px 4px;	-webkit-transition: all .5s ease-in-out;	-moz-transition: all .5s ease-in-out;	-ms-transition: all .5s ease-in-out;	-o-transition: all .5s ease-in-out;	transition: all .5s ease-in-out;}/*=== Breadcrumb ===*/.breadcrumb {	margin: 0;	padding: 0 20px;	background-color: transparent;}.breadcrumb li.active a, .breadcrumb li.active a:hover, .breadcrumb li.active a:focus {	color: #999999;	cursor: default;}.breadcrumb li.active span.divider {	display: none;}/*=== Nav ===*/.nav{	margin: 0;}.nav li{	line-height: normal;	height: auto;}.nav li a{	padding: 3px 6px;	border-radius: 3px;}.nav li.active a,.nav li.active a:hover{	background-color: #0064CB;}/*=== left-nav-tabs ===*/.tabs-left .nav-tabs {	position: absolute;	z-index: 300;	right: 1px;	border: none;	margin: 0;}.tabs-left .nav-tabs a {	padding-left: 30px;	padding: 8px 20px;}.tabs-left .nav-tabs .active a, .tabs-left .nav-tabs .active a:hover {	background-color: white;	color: black;}/*=== functionContainer ===*/#functionContainer{	margin: 0 10px;	min-width: 1155px;}#functionContainer .span3{	max-width: 275px;}#functionContainer .section-top{	border-bottom: 1px solid #EEE;	padding: 0;}#functionContainer .section-top .nav a{	padding: 5px;	margin: 0 5px;	border-radius: 0;}#functionContainer .section-top .nav > .active > a{	background-color: transparent;}#functionContainer .section-header-wrap{	height: 70px;		padding-left: 40px;	position: relative;}#functionContainer .section-header-wrap > .func-ico-back{	position: absolute;	left: 0;	top: 15px;}#functionContainer .section-nav-wrap{	border-left: 1px solid #EEE;	padding: 0;}#functionContainer h2{	font-size: 1.4em;	width: 90%;	margin-left: 10px;	text-align: center;	line-height: 64px;	overflow: hidden;	height: 64px;}#functionContainer h4{	color: #545454;	font-size: 1.15em;	margin: 10px 5px 5px;}/*=== accordion ===*/#functionContainer div{	position:static;}#functionContainer .accordion-inner{	padding: 0;	max-height: 450px;	overflow-y: auto;}#functionContainer .accordion-inner a{	border-radius: 0;}#functionContainer .accordion-group{	border-color: #DBDBDB;}#functionContainer .accordion-toggle{	padding: 5px 10px;	color: #0F52CD;	font-size: 1.1em;}#functionContainer .active a{	color: #050F2E;}#functionContainer .accordion-inner .nav li a{	color: #4D4D4D;	padding: 2px 10px;}#functionContainer .accordion-inner .nav li a:hover{	background-color: #0088cc;	outline: 0;	color: white;}/*=== form ===*/.red-char{	color: red;	font-size:1.2em;}.form-wrap label .red-char{	position: absolute;	right: 3px;}.form-wrap input[type="text"]{	padding: 8px 5px;	box-shadow: 0px 1px 6px rgba(0,0,0,.1) inset;	min-width: 280px;}.form-wrap .form-actions{	background-color: white;	margin:10px 0 0 0;	border: none;}.form-wrap .control-group{	margin-bottom: 10px;}.form-wrap .control-label {	display: inline-block;	text-align: right;	padding:0 15px 0 0;	color: #272727;	position: relative;	vertical-align: top;	top: 8px;}.form-horizontal span.desc{	color: #B53B23;	font-weight: normal;	font-size: 1em;}.page-header{	border:none;}.page-header small{	color: #B53B23;	font-weight: normal;	font-family: Arial, Helvetica, sans-serif;	font-size: 12px;}.modal .control-label,#functionContainer .control-label{	max-width: 100px;}.modal .controls,#functionContainer .form-wrap .controls{	margin-left: 120px;}#functionContainer select{	display: inline-block;	margin-top: 3px;}#functionContainer .control-label input[type="checkbox"],#functionContainer .control-label input[type="radio"]{	position: absolute;}#functionContainer .form-wrap .close{	display: none;	padding: 10px 15px;	background-color: #C61F1F;	color: white;	border-radius: 0 4px 0 4px;}#functionContainer .form-wrap.create .close{	display: none;}#functionContainer .form-wrap.update .close{	display: block;}#functionContainer .form-wrap .close:hover{	opacity: .7;}#functionContainer .form-wrap .form-actions {	background-color: white;	text-align: right;	position: relative;	padding-right: 40px;}#functionContainer .form-wrap{	border: 1px solid #DBDBDB;	border-radius: 4px;	margin-top: 34px;	width: 100%;	position: relative;	overflow-y: auto;	overflow-x: hidden; 	max-height: 600px;}#functionContainer .form-wrap h4{	color:#222222;	margin: 10px 0 10px 20px;	display: inline-block;	font-size: 1.3em;}#functionContainer .form-wrap select{	width: auto;	display: inline-block;}#functionContainer .form-wrap .drag-text{	height: 40px;	border: 2px dashed #1F62C4;	display: inline-block;	min-width: 291px;}#functionContainer .form-wrap .drag-textarea{	padding: 0;	border: 2px dashed #1F62C4;	display: inline-block;	box-sizing: border-box;	min-width: 291px;	min-height: 150px;	margin-top: 5px;}#functionContainer .form-wrap .drag-textarea .select-wrap{	position: absolute;	top: 0;	right: 31px;	margin: 0;	padding: 0 5px;	height: 36px;	line-height: 30px;	font-size: 12px;}#functionContainer .form-wrap .drag-textarea .select-wrap select{	padding: 0;	height: 24px;	font-size: 12px;}.error input[type="text"]:focus,.error textarea:focus{	background-color: transparent;}.error input[type="text"],.error textarea{	border: 1px solid red;	background-color: #F59593;}#functionContainer .form-wrap .error .drag-text,#functionContainer .form-wrap .error .drag-textarea{	border:2px dashed red;	background-color: #F59593;}#functionContainer .form-wrap .drag-text li > a,#functionContainer .form-wrap .drag-textarea li > a{	display:block;	margin: 2px;	position: relative;	height: 30px;	line-height: 30px;	background-color: #0088cc;	border-radius: 0;	color: white;}#functionContainer .form-wrap .drag-text li > a .remove,#functionContainer .form-wrap .drag-textarea li > a .remove{	position: absolute;	padding-left:10px;	padding-right:10px;	display: block;	background-color: #ED3636;	border-radius: 0;	right: 0;	top: 0;	font-weight: bold;	font-size: 20px;	color: white;}#functionContainer .form-wrap .drag-text li > a .remove:hover,#functionContainer .form-wrap .drag-textarea li > a .remove:hover{	background-color: #D62424;}/*=== func-ico ===*/#functionNav li a{	margin-right: 20px;	text-align: center;}#functionNav li a h4{	color: #949494;	font-family: "microsoft yahei";	font-weight: normal;	line-height: normal;	margin: 0;}#functionNav li.active a h4{	color: #357BC8;}.func-ico {	display: inline-block;	vertical-align: bottom;	width: 35px;	height: 40px;	background-image: url(../../img/omp-ico.png);	background-repeat: no-repeat;}.active .func-ico-analysis {	background-position: 0px 0px;}.active .func-ico-query {	background-position: -37px 0px;}.active .func-ico-location {	background-position: -75px 0px;}.active .func-ico-edit {	background-position: -110px 0px;}.active .func-ico-eye {	background-position: -142px 0px;}.func-ico-analysis {	background-position: 0px -45px;}.func-ico-query {	background-position: -37px -43px;}.func-ico-location {	background-position: -76px -44px;}.func-ico-edit {	background-position: -109px -40px;}.func-ico-eye {	background-position: -142px -42px;}.func-ico-back {	width: 39px;	height: 39px;	display: inline-block;	background: url(../../img/omp-ico.png) no-repeat 0px -88px;}.func-ico-back:hover{	opacity: .8;}/*=== table in modal ===*/.modal table td{	padding: 5px 8px;}.large.modal{	width: 600px;	margin-left: -300px;}.large.modal .modal-body{	max-height: 600px;}