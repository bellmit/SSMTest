package cn.gtmap.msurveyplat.exchange.core.entity;



import java.util.List;

/**
 * 消息中心信息
 *
 * @author <a href="mailto:lisongtao@gtmap.cn">lisongtao</a>
 * @version 1.0  2019/2/27.
 * @description
 */
public class BdcMessageDTO {
    /*
   *  待办数量
   * */
    private Integer wbSize;

    /*
  *  待办数量
  * */
    private Integer ybSize;

    /*
   *  未读
   * */
    private List<MessageDto> wdList;
    /*
   *  已读
   * */
    private List<MessageDto> ydList;

    /*
  *  需确认未读信息
  * */
    private List<MessageDto> qrList;

    public List<MessageDto> getWdList() {
        return wdList;
    }

    public void setWdList(List<MessageDto> wdList) {
        this.wdList = wdList;
    }

    public List<MessageDto> getYdList() {
        return ydList;
    }

    public void setYdList(List<MessageDto> ydList) {
        this.ydList = ydList;
    }

    public Integer getWbSize() {
        return wbSize;
    }

    public void setWbSize(Integer wbSize) {
        this.wbSize = wbSize;
    }

    public Integer getYbSize() {
        return ybSize;
    }

    public void setYbSize(Integer ybSize) {
        this.ybSize = ybSize;
    }

    public List<MessageDto> getQrList() {
        return qrList;
    }

    public void setQrList(List<MessageDto> qrList) {
        this.qrList = qrList;
    }
}
