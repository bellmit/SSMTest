@import "map-anim.css";

* {
    margin: 0;
    list-style: none
}

ol, ul {
    padding: 0
}

html, body {
    font-family: "Hiragino Sans GB", "Microsoft Yahei", 微软雅黑, Pinghei, sans-serif;
    font-weight: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    height: 100%;
    width: 100%;
    margin: 0;
    padding: 0;
    min-height: 100%;
    min-width: 990px;
    overflow: hidden;
    *overflow-y: hidden
}

select {
    height: 30px;
    line-height: 30px;
    padding: 5px;
    font-size: 13px;
    border-radius: 3px;
    color: #666;
    vertical-align: middle;
    background-color: #fff;
    border: 1px solid #ccc;
    width: auto;
    min-width: 90px
}

::-webkit-scrollbar {
    width: 10px;
    height: 10px
}

::-webkit-scrollbar-button:vertical {
    display: none
}

::-webkit-scrollbar-corner, ::-webkit-scrollbar-track {
    background-color: #e2e2e2
}

::-webkit-scrollbar-thumb {
    border-radius: 0;
    background-color: rgba(0, 0, 0, 0.3)
}

::-webkit-scrollbar-thumb:vertical:hover {
    background-color: rgba(0, 0, 0, 0.35)
}

::-webkit-scrollbar-thumb:vertical:active {
    background-color: rgba(0, 0, 0, 0.38)
}

::-webkit-input-placeholder {
    font-style: italic;
    font-size: 12px;
    color: #999
}

/* bootstrap reset start */

label {
    font-weight: 500;
    color: #333 !important;
    font-size: 1em !important;
}
.btn {
    border-radius: 0px !important;
    padding: 5px 15px !important;
}

.btn-sm {
    padding: 5px 10px!important;
}

.btn-bordered {
    border-color: #4285f4;
    color: #4285ff
}

.btn .iconfont {
    margin-right: 4px;
}

.btn-primary {
    background-color: #4285f4;
}

.tooltip-inner {
    background-color: #34495E !important;
}

.tooltip.bottom .tooltip-arrow {
    border-bottom-color: #34495E;
}

.nav-tabs {
    border: 0px !important;
    margin: 12px 6px;
}
.nav-tab {
    width: 50%;
    text-align: center
}
.nav-tabs>li {
    margin-bottom: 0px;
}

.nav-tabs>li>a {
    margin-right: 0px;
    background: #d9e7fd;
    border: 0px !important;
    border-radius: 0px !important
}

.nav-tabs>li.active>a, .nav-tabs>li.active>a:focus, .nav-tabs>li.active>a:hover {
    color: #fff;
    background-color: #4285f4;
    border: 0px !important;
    border-radius: 0px !important;
}

.tooltip-inner {
    min-height: 26px !important;
}

.panel {
    border-radius: 0px !important;
}

.panel-heading {
    border-radius: 0px !important;
    border-bottom: none;
    height: 35px;
    padding: 8px 15px !important;
}

.panel-primary .panel-heading {
    color: #fff;
    background-color: #4285f4;
    border-color: #4285f4;
}

.panel-primary .panel-heading a {
    color: #fff;
}
.panel-primary .panel-heading a:hover {
    color: #d9e7fd;
}
/* bootstrap reset end */

a:hover, a:focus {
    text-decoration: none
}

.m-t-5 {
    margin-top: 5px
}

.m-t-10 {
    margin-top: 10px
}

.m-l-5 {
    margin-left: 5px
}

.m-l-10 {
    margin-left: 10px
}

.m-r-5 {
    margin-right: 5px
}

.m-r-10 {
    margin-right: 10px
}

.m-r-48 {
    margin-right: 48px !important
}

.usel {
    user-select: none;
    -ms-user-select: none
}

.tooltip {
    opacity: .8 !important;
    filter: progid:DXImageTransform.Microsoft.Alpha(opacity=80);
}

.esriPopupWrapper {
    user-select: initial;
    -ms-user-select: initial
}

.coords-covert {
    cursor: pointer;
    display: none
}

.coords-covert:hover {
    color: #0288d1
}

.nav-topbar {
    display: none;
    height: 70px;
    padding: 12px 10px;
    min-width: 1000px;
    border-bottom: 1px solid #ddd;
    background: url("./img/bg_navbar.png") no-repeat;
}

.nav-topbar a {
    color: #F2F2F2;
}

.nav-topbar .m-logo {
    float: left;
    width: 46px;
    height: 47px;
    display: block;
    background: url(./img/mlogo.png) no-repeat;
    vertical-align: middle;
    background-size: contain
}

.nav-topbar .nav-title {
    display: none;
    font-size: 1.8em;
    color: #ffffff;
    line-height: 46px;
    /* margin-left: 8px; */
}

.sys-login {
    float: right;
    line-height: 46px;
    line-height: 46px;
    cursor: pointer
}
.sys-login a i {
    font-size: 16px;
}
.sys-login .user-info {
    font-size: 15px;
}

.nav-topbar a:hover {
    text-decoration: none
}

.nav-topbar .sys-search {
    float: right;
    width: 283px;
    height: 30px;
    line-height: 30px;
    margin: 8px;
    padding: 0 8px;
    border: none;
    background: url("./img/nav_search.png") no-repeat;

}

.nav-topbar .sys-search .sys-search-input {
    border: 0;
    background: 0;
    color: #555;
    width: 82%;
    height: 30px;
    font-size: 13px;
    outline: 0
}

.nav-topbar .sys-search .sys-search-btn {
    border: 0;
    background: 0;
    color: #bbb;
    cursor: pointer;
    text-decoration: none;
    float: right
}

.nav-topbar .sys-search .sys-search-btn:hover {
    text-decoration: none;
    color: #0288d1
}

.nav-topbar .sys-search .sys-search-btn i {
    font-size: 17px
}


.sys-menu {
    position: absolute;
    right: 120px;
    top: 30px;
    width: 420px;
    height: 35px;
    line-height: 35px;
    margin-top: 5px
}

.sys-menu a {
    cursor: pointer;
    font-size: 15px;
    margin-left: 1px;
    text-align: center;
    text-decoration: none;
    color: #666;
    padding: 0 8px;
    display: block;
    float: left
}

.sys-menu a:hover {
    color: #808080;
    border-bottom: 2px solid #808080
}

.sys-menu ul {
    display: none;
    position: absolute;
    z-index: 1000;
    top: 100%;
    left: 108px;
    width: 90px;
    padding: 0;
    border: 1px solid #d8d5d5;
    height: auto
}

.sys-menu ul li {
    z-index: 20;
    background: #fff;
    height: 30px;
    text-align: center;
    line-height: 30px;
    display: block
}

.sys-menu ul li a {
    width: 100%;
    color: #666;
    float: none;
    text-decoration: none
}

.sys-menu ul li a:hover {
    display: block;
    color: #808080
}

.sys-menu > a[class~="active"], .sys-menu > a[class~="active"]:hover {
    text-decoration: none;
    color: #0288d1;
    border-bottom: 2px solid #0288d1
}

.grey {
    color: #a9a9a9
}

.main-operation {
    width: 100%;
    /* max-width: 460px; */
    position: relative;
    transition: all .2s ease-in-out 0s;
    z-index: 3
}
#popupwin_area{
    width: 100%;
}
.main-operation-handle {
    display: none;
    width: 21px;
    position: absolute;
    z-index: 100;
    cursor: pointer;
    text-decoration: none;
    text-align: center;
    font-size: 18px;
    margin: 4px;
}

.main-operation-handle:hover {
    color: #3385ff
}

.vertical-operation-menu {
    padding-top: 5px;
    width: 46px;
    border-right: 1px solid #ececec;
    float: left;
    min-height: 200px;
    background: #d9e7fd;
}

.optbtn,
.groupbtn {
    display: block;
    cursor: pointer;
    text-align: center;
    text-decoration: none;
    color: #333333;;
    font-size: 1em;
    position: relative;
    border-bottom: 1px solid #ebf2f9
}

.groupbtn:hover {
    background-color:#4285f4!important;
    color: #ffffff;
}

.optbtn > .widget-title,
.groupbtn > .widget-title {
    padding: 30px 2px 2px 2px;
    display: inline-block;
}

.group-widget-btn-active,
.group-widget-btn:hover{
    background-color:#4285f4;
    color: #ffc107!important;
    font-size: 14px;
}

.widget-group-content{
    position: absolute;
    width: 140px;
    z-index: 9999;
    left: 45px;
    top: 0px;
    background-color: #4285f4;
    margin: 0;
    display: none;
}

.widget-group-content>li{
    width:100%;
    text-align: left;
    margin: 0;
    border: 0;
    color: #FFFFFF;
    background-color: #4285f4;
}

.optbtn:last-child {
    border-bottom: 0
}

.optbtn:hover {
    background-color: #4285f4!important;
    color: #fff;
    text-decoration: none
}

.optbtn .close-this {
    position: absolute;
    top: -2px;
    right: -1px;
    z-index: 100;
    display: none
}

.optBtnActive:hover, .optBtnActive {
    background-color: #4285f4!important;
    color: #fff
}
.popup-Statistics-win{
    width: 100%;
    display: none;
}
.popup-window {
    min-width: 300px;
    float: left;
     background: #fff; 
    border-right: 1px solid #dcdcdc;
    padding: 6px;
    z-index: 100;
    position: relative;
    box-sizing: border-box;
}

.popup-window .popup-close-btn {
    cursor: pointer;
    top: 0;
    right: 6px;
    color: #bbb;
    position: absolute;
    font-size: 18px
}

.popup-window .popup-close-btn:hover {
    color: #0288d1
}

.widget-panel-title {
    color: #333;
    padding: 0 10px 5px 10px;
    font-size: 1.2857em;
    border-bottom: 1px solid #d9e7fd;
    font-weight: 400
}
.widget-panel-title span{
    color: #999999;
    font-size: 13px;
}

.panel-body {
    height: calc(100% -32px);
    padding: 5px;
    box-sizing: border-box;
}

#result-container {
    display: none;
    position: absolute;
    bottom: 0;
    text-align: center;
     background: #fff; 
    border-top: 1px solid #dcdcdc;
    opacity: .94;
    z-index: 100
}

#result-container .shrink {
    float: left;
    width: 100%;
    position: relative;
    height: 30px;
    top: -29px
}

#result-container .btn-default {
    border-bottom: 0;
    opacity: .7;
    filter: alpha(opacity=70)
}

#result-container .btn-default:hover {
    background: #fff;
    opacity: 1;
    filter: alpha(opacity=100)
}

#result-container .content {
    width: 100%;
    min-height: 235px;
    padding: 5px
}

#result-container iframe {
    position: absolute;
    left: 0;
    top: 5px
}

#result-container .expand {
    display: none
}

.shrinked {
    background: none !important;
    border-top: none !important
}

/* 右侧 */
.right_panel {
    display: none;
    width: 400px;
     background: #fff; 
    border-left: 1px solid #dcdcdc;
    padding: 5px;
    position: absolute;
    right: 1px
}

#map-content {
    position: absolute;
    right: 0;
    top: 70px;
    z-index: 0;
    width: 100%;
    min-width: 400px;
    background: #eff2f7;
    overflow: hidden
}

#main-map {
    width: 100%;
    height: 100%;
    right: 0;
    z-index: 1
}

.contrast-map {
    float: left;
    display: none;
    z-index: 1;
    margin: 1px
}

.selected-contrast .contrast-map-close {
    color: #ea3a36
}

.contrast-map-close {
    font-size: 30px;
    cursor: pointer;
    margin-right: 2px;
    float: right;
    z-index: 1000;
    position: relative;
    color: #bbb
}

.contrast-map-close:hover {
    color: #ea3a36
}

.contrast-border {
    border: 2px solid #0288d1
}

.contrast-main-map {
    float: left;
    margin: 1px
}

.selected-contrast {
    border: 2px solid #ea3a36 !important
}

#basemapToggle {
    position: absolute;
    z-index: 50;
    bottom: 50px;
    padding-left: 10px
}

.BasemapToggle .basemapTitle {
    width: 100% !important
}

.BasemapToggle .toggleButton {
    border-radius: 0 !important;
    border: 1px solid #9e9e9e !important
}

.BasemapToggle .basemapTitle {
    background: #525568 !important;
    color: #fff !important;
    border-radius: 0 !important
}

#coordsInfo {
    position: absolute;
    z-index: 50;
    bottom: 10px;
    color: #666;
    padding-left: 10px;
    font-size: 13px;
    background: rgba(255, 255, 255, 0.7)
}

.dot-place-holder {
    display: inline-block;
    width: 8px;
    height: 8px;
    margin: 0 6px;
    -webkit-border-radius: 4px;
    border-radius: 4px;
    border: 1px solid #b7c4d4
}

.Validform_wrong {
    color: red;
    white-space: nowrap
}

.Validform_checktip {
    line-height: 20px;
    height: 20px;
    overflow: hidden;
    font-size: 12px
}

.no-data {
    padding: 8px
}

.info-box {
    display: block;
    min-height: 60px;
    background: #fff;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
    border-radius: 4px;
    border: 1px solid #ccc;
    margin: 0px 0px 10px;
}

.info-box-opt {
    float: right;
    line-height: 55px;
    padding-right: 10px;
    font-size: 24px
}

.info-box-opt span {
    font-size: inherit;
    cursor: pointer
}

.info-box-opt span:first-child {
    margin-right: 6px
}

.info-box-content {
    padding: 5px 10px;
    margin-right: 24px
}

.info-box-title {
    display: block;
    font-size: 15px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    margin-bottom: 6px
}

.info-box-subtitle {
    display: block;
    font-size: 12px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis
}

#scaleInfo {
    display: block;
    z-index: 50;
    bottom: 28px;
    color: #666;
    padding-left: 10px;
    position: absolute;
    font-size: 13px
}

.zoom-slider {
    display: none;
    opacity: .7;
    filter: alpha(opacity=70);
    box-shadow: 0 2px 6px rgba(0, 0, 0, 0.3)
}

.zoom-slider:hover, .zoom-slider:focus, .zoom-slider:active {
    opacity: 1
}

.zoom-slider.ui-slider {
    background: #fff;
    border: 0;
    height: 200px;
    width: 6px;
    margin-left: 8px
}

.zoom-control .ui-corner-all {
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-top-left-radius: 0
}

.zoom-slider.ui-widget-content .ui-slider-handle.ui-state-default {
    background: #0288d1
}

.zoom-slider .ui-slider-pip {
    left: 4px
}

.zoom-slider.ui-slider.ui-slider-vertical .ui-slider-pip {
    left: 10px
}

.zoom-slider.ui-slider.ui-slider-vertical .ui-slider-pip .ui-slider-line {
    height: 2px;
    width: 10px;
    margin-top: 3px;
    margin-left: -3px;
    background: #0288d1 !important
}

.zoom-slider.ui-slider.ui-slider-vertical .ui-slider-pip[class*=ui-slider-pip-selected] .ui-slider-line {
    width: 14px;
    background: #3280fc
}

.zoom-slider.ui-slider.ui-slider-vertical .ui-slider-pip.ui-slider-pip-inrange .ui-slider-line {
    background: #3280fc
}

.zoom-slider .ui-slider-pip.ui-slider-pip-last .ui-slider-line, .zoom-slider .ui-slider-pip.ui-slider-pip-first .ui-slider-line {
    display: none
}

.zoom-slider .ui-slider-pip.ui-slider-pip-last .ui-slider-label, .zoom-slider .ui-slider-pip.ui-slider-pip-first .ui-slider-label {
    margin: 0
}

.zoom-slider .ui-slider-pip.ui-slider-pip-first .ui-slider-label {
    left: -2em;
    text-align: right
}

.zoom-slider .ui-slider-pip.ui-slider-pip-last .ui-slider-label {
    left: 2em;
    text-align: left
}

.zoom-slider .ui-slider-pip.ui-slider-pip-selected-initial {
    font-weight: normal
}

.zoom-slider .ui-slider-pip.ui-slider-pip-selected {
    font-weight: bold
}

.zoom-slider .ui-slider-pip.ui-slider-pip-selected, .zoom-slider .ui-slider-pip.ui-slider-pip-selected-initial {
    color: #0288d1
}

.zoom-slider.ui-widget-content .ui-slider-handle.ui-state-active, .zoom-slider.ui-widget-content .ui-slider-handle.ui-state-focus, .zoom-slider.ui-widget-content .ui-slider-handle.ui-state-hover {
    background: #3280fc
}

.zoom-slider.ui-slider .ui-slider-range {
    z-index: 0;
    background: #3280fc
}

.zoom-slider.ui-slider-vertical.ui-slider-pips {
    margin-bottom: 0 !important
}

.zoom-slider.ui-slider-vertical.ui-slider-pips .ui-slider-pip {
    margin-bottom: 0 !important
}

.zoom-slider.ui-slider-vertical .ui-slider-handle {
    width: 18px;
    height: 8px;
    border: 1px solid #fffaf7;
    margin-bottom: -0.15em !important;
    left: -0.4em !important;
    cursor: pointer
}

.zoom-control {
    width: 28px;
    padding: 0 2px;
    margin: 1em 1em 1em 4.5em;
    text-align: center
}

.map-control {
    z-index: 10;
     background: #fff; 
    color: #666;
    cursor: pointer;
    border: 1px solid #ebf2f9;
    height: 23px;
    line-height: 19px;
    width: 24px;
    box-shadow: 0 2px 6px rgba(0, 0, 0, 0.3)
}

.map-control:hover {
    color: #3385ff
}

.map-control:hover {
    opacity: 1
}

.map-control.zoom-unable:hover, .zoom-unable {
    cursor: default;
    opacity: .7
}

.map-globe {
    margin-bottom: 10px
}

.omp-switch {
    position: relative;
    height: 24px;
    line-height: 24px;
    width: 54px;
    padding: 0 5px;
    border: 1px solid #d2d2d2;
    border-radius: 20px;
    cursor: pointer;
    background-color: #fff;
    -webkit-transition: .1s linear;
    transition: .1s linear
}

.omp-switch i {
    position: absolute;
    left: 5px;
    top: 3px;
    width: 16px;
    height: 16px;
    border-radius: 20px;
    background-color: #d2d2d2;
    -webkit-transition: .1s linear;
    transition: .1s linear
}

.omp-switch em {
    position: absolute;
    right: 5px;
    top: 0;
    width: 25px;
    height: 22px;
    line-height: 22px;
    padding: 0 !important;
    text-align: center !important;
    color: #999 !important;
    font-style: normal !important;
    font-size: 12px
}

.omp-switch-on {
    border-color: #0288d1;
    background-color: #0288d1
}

.omp-switch-on i {
    left: 32px !important;
    background-color: #fff !important
}

.omp-switch-on em {
    left: 5px !important;
    right: auto !important;
    color: #fff !important
}

.omp-checkbox {
    display: inline-block;
    vertical-align: middle;
    position: relative;
    height: 30px;
    line-height: 28px;
    margin-right: 10px;
    padding-right: 30px;
    border: 1px solid #d2d2d2;
    cursor: pointer;
    font-size: 0;
    border-radius: 2px;
    -webkit-transition: .1s linear;
    transition: .1s linear;
    box-sizing: border-box !important
}

.omp-checkbox * {
    display: inline-block;
    vertical-align: middle
}

.omp-checkbox:hover {
    border: 1px solid #c2c2c2
}

.omp-checkbox:hover span {
    background-color: #c2c2c2
}

.omp-checkbox:hover i {
    color: #c2c2c2
}

.omp-checkbox span {
    padding: 0 10px;
    height: 100%;
    font-size: 14px;
    background-color: #d2d2d2;
    color: #fff;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis
}

.omp-checkbox i {
    position: absolute;
    right: 0;
    width: 30px;
    color: #fff;
    font-size: 20px;
    text-align: center
}

.omp-checked, .omp-checked:hover {
    border-color: #0288d1
}

.omp-checked span, .omp-checked:hover span, .omp-checked span:hover, .omp-checked:hover span:hover {
    background-color: #0288d1
}

.omp-checked i, .omp-checked:hover i, .omp-checked i:hover, .omp-checked:hover i:hover {
    color: #0288d1
}

.form-group label {
    font-weight: normal !important
}

.default-icon{
    background: url("./img/default.png") no-repeat;
}

.default-icon:hover {
    background: url("./img/default_hover.png") no-repeat;
}

.analysis-group-icon{
    background: url("./img/widget-icons/analysis_group.png") no-repeat top;
}
.analysis-group-icon:hover{
    background: url("./img/widget-icons/analysis_group_hover.png") no-repeat top;
}