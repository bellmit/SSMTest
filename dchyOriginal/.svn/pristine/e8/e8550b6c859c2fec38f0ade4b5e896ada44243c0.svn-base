.title {
    font-size: 14px;
    font-weight: 500;
    font-family: 宋体;
}

.split-line {
    background-color: #408FC6;
    height: 2px;
    width: 100%;
}

.scroll{
    overflow-y: auto;
    height: 350px;
    padding-bottom: 5px;
}
.scroll-none-ie{
    overflow-y: auto;
    padding-bottom: 5px;
}


.all-width {
    width: 100%;
}

.all-width-content {
    width: 100%;
    margin-top: 5px;

}

.pro-alias {
    width: 30%;
    float: left;
    height: 20px;
    text-align: right;
    overflow: hidden;
    text-overflow: ellipsis;

}

.pro-value {
    max-width: 60%;
    float: left;
    height:20px;
    overflow: hidden;
    text-overflow: ellipsis;
}


.super-link {
    float: right;
    padding-right: 20px;
}

.esriPopup .contentPane{
    overflow: hidden!important;
}