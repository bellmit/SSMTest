    /*background:         linear-gradient(top, #0091FF  0%, #42B5EC 100%);*/
    background: -webkit-linear-gradient(top, #67d0fe 0%, #42B5EC 100%);
    background:    -moz-linear-gradient(top, #67d0fe  0%, #42B5EC 100%);
    background:         linear-gradient(top, #67d0fe  0%, #42B5EC 100%);
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
}

.dd3-handle:before {content: '\f085'; font-family: FontAwesome; display: block; position: absolute; left: 0; top: 3px; width: 100%; text-align: center; text-indent: 0; color: #fff; font-size: 14px; font-weight: normal; }

.dd3-handle1:before {content: '\f02c'; font-family: FontAwesome; display: block; position: absolute; left: 0; top: 3px; width: 100%; text-align: center; text-indent: 0; color: #fff; font-size: 14px; font-weight: normal; }

.dd3-handle2:before {content: '\f02b'; font-family: FontAwesome; display: block; position: absolute; left: 0; top: 3px; width: 100%; text-align: center; text-indent: 0; color: #fff; font-size: 14px; font-weight: normal; }

