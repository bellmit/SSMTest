/* Radio */

input[type="radio"] {
    border-radius: 50%;
    border: 1px #4baf4f solid;
    cursor: pointer;
    display: inline-block;
    height: 17px;
    margin-right: 15px;
    position: relative;
    width: 17px;
    -webkit-appearance: none;
    box-shadow: 0px 1px 1px rgba(0, 0, 0, 0.25);
}

input[type="radio"]:after {
    border-radius: 50%;
    content: '';
    display: block;
    height: 9px;
    left: 3px;
    position: relative;
    top: 3px;
    width: 9px;
}

input[type="radio"]:checked:after {
    background: #4baf4f;
    border: 1px #4baf4f solid;
}

