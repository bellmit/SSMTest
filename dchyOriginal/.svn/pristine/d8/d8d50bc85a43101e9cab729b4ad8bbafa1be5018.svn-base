/*-------------------------------------
zTree Style using iconfont instead of images

-------------------------------------*/
.ztree * {
  padding: 0;
  margin: 0;
}

.ztree {
  height: 100%;
  color: #333333;
  position: relative;
}

.ztree li {
  text-align: left;
  white-space: nowrap;
  outline: 0;
  position: relative;
}

/*根节点*/
.ztree >li {
  font-size: 1.0714em;
  padding: 1em 0 0.3em 0;
  border-bottom: 1px dotted #ababab;
}

.ztree li span{
  height: 20px;
  line-height: 20px;
  margin-right: 8px;
  display: inline-block;
}

/*按钮*/
.ztree li span .button {
  vertical-align: top;
  cursor: pointer;
  outline: none;
  background-color: #ffffff;
}

.ztree .level0 .root_open, .ztree .level0 .roots_open,.ztree .level0 .center_open, .ztree .level0 >.bottom_open {
  width: 20px !important;
  background: url("./img/roots_open.png") no-repeat center !important;
  margin-left: 0px !important;
  margin-right: 8px !important;
}

.ztree .level0 .root_close, .ztree .level0 .roots_close,.ztree .level0 .center_close, .ztree .level0 >.bottom_close {
  width: 20px !important;
  background: url("./img/roots_close.png") no-repeat center !important;
  margin-left: 0px !important;
  margin-right: 8px !important;
}

.ztree li span.button.bottom_open {
  margin-right: 2px;
  width: 20px;
  background: url("./img/roots_open.png") no-repeat center;
}

.ztree li span.button.bottom_close {
  margin-right: 2px;
  width: 20px;
  background: url("./img/roots_close.png") no-repeat center;
}

.ztree li span.button.leafRegionCamera_ico_docu {
  width: 20px !important;
  background: url("./img/view_camera.png") no-repeat center !important;
  margin-left: 0px !important;
  margin-right: 8px !important;
}

.ztree li span.button.leafRegion_ico_docu {
  width: 20px !important;
  background: url("./img/roots_close.png") no-repeat center !important;
  margin-left: 0px !important;
  margin-right: 8px !important;
}


.ztree .node_name {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  display: inline-block;
  max-width: 180px;
}
/*二级节点*/
.ztree >li>ul>li>a{
  font-size: 1em;
}

/*除根节点外的节点*/
.ztree ul li a{
  font-size: 14px;
  width: 100%;
}



.ztree li ul {
  margin: 0px;
  padding: 0 0 0 8px;
}

/*根节点*/
.ztree li a {
  padding-right: 3px;
  margin: 0;
  cursor: pointer;
  color: #333333;
  background-color: transparent;
  text-decoration: none;
  vertical-align: top;
  display: inline-block;
  font-weight: 500;
  font-size: 1.0714em;
}


.ztree li span.button.chk:before {
  color: #4285f4;
  font-family: FontAwesome;
  font-size: 1.18em;
}
.ztree li span.button.chk {
  padding: 2px 0px;
  margin: 0;
  display: inline-block;
  position: absolute;
  /*float: right;*/
  right: 1px;
}
.ztree li span.button.chk.checkbox_false_full:before {
  content: "\f096";
}
.ztree li span.button.chk.checkbox_false_full_focus:before {
  content: "\f096";
}
.ztree li span.button.chk.checkbox_false_part:before {
  content: "\f096";
  color: #aaaaaa;
}
.ztree li span.button.chk.checkbox_false_part_focus:before {
  content: "\f096";
  color: #cad96c;
}
.ztree li span.button.chk.checkbox_false_disable:before {
  content: "\f096";
  color: #808080;
}

.ztree li span.button.chk.checkbox_true_full:before {
  content: "\f14a";
  before: "\f14a";
}
.ztree li span.button.chk.checkbox_true_full_focus:before {
  content: "\f14a";
}
.ztree li span.button.chk.checkbox_true_part:before {
  content: "\f14a";
}
.ztree li span.button.chk.checkbox_true_part_focus:before {
  content: "\f14a";
}
.ztree li span.button.chk.checkbox_true_full_focus:before {
  content: "\f14a";
}
.ztree li span.button.chk.checkbox_true_part:before {
  content: "\f14a";
  color: #aaaaaa;
}
.ztree li span.button.chk.checkbox_true_part_focus:before {
  content: "\f14a";
}
.ztree li span.button.chk.checkbox_true_disable:before {
  content: "\f14a";
  color: #808080;
}
.ztree li span.button.chk.radio_false_full:before {
  content: "\f10c";
}
.ztree li span.button.chk.radio_false_full_focus:before {
  content: "\f10c";
}
.ztree li span.button.chk.radio_false_part:before {
  content: "\f10c";
  color: #aaaaaa;
}
.ztree li span.button.chk.radio_false_part_focus:before {
  content: "\f10c";
}
.ztree li span.button.chk.radio_false_disable:before {
  content: "\f1db";
  color: #808080;
}
.ztree li span.button.chk.radio_true_full:before {
  content: "\f192";
}
.ztree li span.button.chk.radio_true_full_focus:before {
  content: "\f192";
}
.ztree li span.button.chk.radio_true_part:before {
  content: "\f192";
  color: #aaaaaa;
}
.ztree li span.button.chk.radio_true_part_focus:before {
  content: "\f192";
  color: #aaaaaa;
}
.ztree li span.button.chk.radio_true_disable:before {
  content: "\f1db";
  color: #808080;
}

/*叶子节点*/
.ztree li span.button .roots_docu, .ztree li span.button.ico_docu{
  width: 12px;
  background: url("./img/roots_docu.png") no-repeat center;
}

.ztree li span.button.noline_docu {
  background: none;
}
.ztree li span.button.ico_open,
.ztree li span.button.ico_close{
  display: none;
}


.ztree li span.button.edit {
  margin-left: 4px;
  margin-right: -1px;
  vertical-align: top;
  *vertical-align: middle;
  padding-top: 10px;
}
.ztree li span.button.edit:before {
  content: "\f044";
  font-family: FontAwesome;
}
.ztree li span.button.remove {
  margin-left: 4px;
  margin-right: -1px;
  vertical-align: top;
  *vertical-align: middle;
  padding-top: 10px;
}
.ztree li span.button.remove:before {
  content: "\f1f8";
  font-family: FontAwesome;
}
.ztree li span.button.add {
  margin-left: 4px;
  margin-right: -1px;
  vertical-align: top;
  *vertical-align: middle;
  padding-top: 10px;
}
.ztree li span.button.add:before {
  content: "\f067";
  font-family: FontAwesome;
}
.ztree li span.button.ico_loading {
  margin-right: 2px;
  background: url(./img/loading.gif) no-repeat scroll 0 0 transparent;
  vertical-align: top;
  *vertical-align: middle;
}
ul.tmpTargetzTree {
  background-color: #FFE6B0;
  opacity: 0.8;
  filter: alpha(opacity=80);
}
span.tmpzTreeMove_arrow {
  width: 16px;
  height: 17px;
  display: inline-block;
  padding: 0;
  margin: 2px 0 0 1px;
  border: 0 none;
  position: absolute;
  background-color: transparent;
  background-attachment: scroll;
}
span.tmpzTreeMove_arrow:before {
  content: "\f04b";
  font-family: FontAwesome;
  color: #ffff00;
}
ul.ztree.zTreeDragUL {
  margin: 0;
  padding: 0;
  position: absolute;
  width: auto;
  height: auto;
  overflow: hidden;
  background-color: #cfcfcf;
  border: 1px #ffff00 dotted;
  opacity: 0.8;
  filter: alpha(opacity=80);
}
.ztreeMask {
  z-index: 10000;
  background-color: #cfcfcf;
  opacity: 0.0;
  filter: alpha(opacity=0);
  position: absolute;
}