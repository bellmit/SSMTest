<div class="widget-panel-title">
    数据录入
</div>
<div class="panel-body">
    <div class="row tools-wrapper">
        <label class="col-xs-3 control-label text-right">录入图层</label>
        <div class="col-md-7 col-xs-11" style="padding: 0px;">
            <span class="text-muted pull-left" style="width: 100%;" id="iDescInsertSpan">无信息</span>
        </div>
    </div>
    <div>

        <div id="drawDiv" style="width: 60%;">
            <div style="float: left;">
                <div class=" display pointDiv">
                    <div class="spatial-geo-icon point-choose " data-type="point"></div>
                    <div class="choose-title">点</div>
                </div>
                <div class=" display polygonDiv">
                    <div class="spatial-geo-icon polygon-choose" data-type="polygon"></div>
                    <div class="choose-title">多边形</div>
                </div>
            </div>
            <div style="float: right;">
                <button id="insertClearBtn" class="btn btn-primary btn-primary xy-search-btn">
                    <i class="iconfont">&#xe605;</i>清除
                </button>
            </div>
        </div>
    </div>
    <div id="inserDataResultPanel" style="margin-top: 80px;"></div>


</div>

<script id="insertAttrResultTpl" type="x-tpl-mustache">
	<div class="row result-info">
	    <span class="text-primary">共 <strong>{{size}}</strong>&nbsp;条记录</span>
	</div>
	<div id="insertAttr_result" style="padding-top:15px;"></div>
	<div id=pageTool class="text-center"></div>

</script>



